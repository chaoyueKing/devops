{

  "workspace": {
    "path": "./workspace"
  },

  "approaches": [

    {
      "processor": "VCSProcessor",
      "ignore": false,
      "configuration": {
        "working_copy": [
          { "dir": "conf", "url": "http://10.10.10.176:88/svn/accountcenter/Devconf/maven/" },
          { "dir": "common", "url": "http://10.10.10.176:88/svn/accountcenter/Code/accountcenter/branches/2.9.7.4/common" },
          { "dir": "suss", "url": "http://10.10.10.176:88/svn/accountcenter/Code/accountcenter/branches/2.9.7.4/common" },
          { "dir": "scos", "url": "http://10.10.10.176:88/svn/accountcenter/Code/accountcenter/branches/2.9.7.4/common" }
        ]
      }
    },

    {
      "processor": "BUILDProcessor",
      "ignore": false,
      "configuration": {
        "mvn_setting": "conf/settings.xml",
        "builds": [
          { "lp": "clean install", "path": "common" },
          { "lp": "clean install", "path": "suss" },
          { "lp": "clean install", "path": "scos" }
        ]
      }
    },

    {
      "processor": "PATCHProcessor",
      "ignore": false,
      "configuration": {
        "base_path": "base",
        "dest_path": "dest",
        "output_path": "output"
      }
    }

  ]

}
